<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBox" alignment="CENTER" prefHeight="57.0" prefWidth="594.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bebra.treevisualization.visual.HelloController">
   <HBox prefHeight="76.0" prefWidth="594.0">
      <children>
         <VBox prefHeight="200.0" prefWidth="200.0">
            <children>
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <children>
                     <TextField fx:id="keyField" />
                     <Label text="Key" />
                  </children>
               </HBox>
               <HBox prefHeight="29.0" prefWidth="164.0">
                  <children>
                     <TextField fx:id="valField" />
                     <Label prefHeight="17.0" prefWidth="49.0" text="Val" />
                  </children>
               </HBox>
            </children>
         </VBox>
         <Button mnemonicParsing="false" onAction="#put" text="Put" />
         <Button mnemonicParsing="false" onAction="#delete" text="Delete" />
         <Button mnemonicParsing="false" onAction="#change" text="Change" />
         <Button mnemonicParsing="false" onAction="#clear" text="Clear" />
      </children>
   </HBox>
</VBox>
